<proxy name="PatientRegistrationJMSProxy"
       transports="jms"
       startOnLoad="true"
       trace="disable"
       xmlns="http://ws.apache.org/ns/synapse">
   <target>
      <inSequence>
         <log level="full">
        <property name="INFO" value="Checking for new patient registrations"/>
    </log>
      </inSequence>
   </target>
   <parameter name="transport.jms.ConnectionFactoryJNDIName">myQueueConnectionFactory</parameter>
   <parameter name="transport.jms.Destination">PatientRegistrationQueue</parameter>
   <parameter name="transport.jms.DestinationType">queue</parameter>
</proxy>